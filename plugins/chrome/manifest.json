{
  "name": "English Scene",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",

    "content_scripts": [ {  
          "exclude_globs": [  ],  
          "include_globs": [ "*" ],  
          "js": [
              "src/lib/jquery-1.9.1.min.js"
              , "src/lib/wordpicker.js"
              , "src/lib/ui.js"
              , "src/lib/main.js"
          ],
        "css": [
            "src/style/style.css"
        ],
          "matches": [ "http://*/*", "https://*/*" ],  
          "run_at": "document_end"  
       } ]
    
}
